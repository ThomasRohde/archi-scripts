{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.ajs$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true,
            "includeDate": false
        }
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true,
        "readme": "./README.md"
    },
    "markdown": {
        "hardwrap": false,
        "idInHeadings": true
    }
  }